<h2>Tests</h2>

<%= render 'shared/errors', object: @test %>
<table class='table'>
  <tr>
    <th class='sort-by-title pointer'>
      <%= t('.name') %>
      <%= octicon 'arrow-up', class: 'text-success-up ' %>
      <%= octicon 'arrow-down', class: 'text-success-down ' %>
    </th>
    <th><%= t('.questions') %></th>
    <th colspan=3><%= t('.action') %></th>
  </tr>
  <%= render @tests %>
</table>
<br>
<div><%= link_to t('.new'), new_admin_test_path %></div>
<div><%= link_to t('.gists'), admin_gists_path %></div>
